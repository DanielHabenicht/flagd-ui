{
  "$schema": "https://flagd.dev/schema/v0/flags.json",
  "flags": {
    "background-color": {
      "defaultVariant": "red",
      "state": "ENABLED",
      "variants": {
        "blue": "#0000FF",
        "green": "#00FF00",
        "red": "#FF0000",
        "yellow": "#FFFF00"
      }
    },
    "show-welcome-banner": {
      "defaultVariant": "off",
      "state": "ENABLED",
      "variants": {
        "off": false,
        "on": true
      }
    },
    "test": {
      "defaultVariant": "on",
      "state": "ENABLED",
      "targeting": {
        "if": [
          {
            ">=": [
              {
                "var": "$flagd.timestamp"
              },
              "2026-02-11T23:30:00.000Z"
            ]
          },
          "off",
          "on"
        ]
      },
      "variants": {
        "off": false,
        "on": true
      }
    }
  }
}